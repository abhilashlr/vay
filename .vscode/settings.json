{
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.exclude": {
    ".next": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "node_modules": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/yarn.lock": true
  }
}
